{
    "name": "phpmob/twig-modify-bundle",
    "description": "Twig Minifier just you like.",
    "keywords": [
        "minifier",
        "javascript",
        "css",
        "html",
        "xss",
        "cssmin",
        "jsmin",
        "htmlpurify"
    ],
    "homepage": "http://github.com/phpmob/twig-modify-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ishmael Doss",
            "email": "nukboon@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "ezyang/htmlpurifier": "^4.9",
        "tedivm/jshrink": "^1.2",
        "tubalmartin/cssmin": "^4.1",
        "voku/anti-xss": "^4.0",
        "symfony/cache": "^3.1|~4.0",
        "symfony/config": "^3.1|~4.0",
        "symfony/dependency-injection": "^3.1|~4.0",
        "symfony/http-kernel": "^3.1|~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4.3"
    },
    "autoload": {
        "psr-4": {
            "PhpMob\\TwigModifyBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpMob\\TwigModifyBundle\\spec\\": "spec/"
        }
    }
}
