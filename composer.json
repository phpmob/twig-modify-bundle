        "minifier",
        "javascript",
        "css",
        "html",
        "xss",
        "cssmin",
        "jsmin",
        "htmlpurify"
    ],
    "homepage": "http://github.com/phpmob/twig-modify-bundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ishmael Doss",
            "email": "nukboon@gmail.com"
        }
    ],
    "require": {
