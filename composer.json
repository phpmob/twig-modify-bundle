        "doctrine/cache": "^1.4.2",
        "ezyang/htmlpurifier": "^4.9",
        "tedivm/jshrink": "^1.2",
        "tubalmartin/cssmin": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "PhpMob\\TwigModifyBundle\\": "src/"
